<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".more.subscribe.fragments.SubscribeFragment">

    <include layout="@layout/title_bar_new" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_weight="1">


            <in.woloo.www.more.subscribe.mvp.EmptyRecyclerView
                android:id="@+id/recyclerView_subscribe"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:listitem="@layout/item_silver_membership"
                android:background="@color/white"
                />

        <in.woloo.www.application_kotlin.utilities.CenturyGoticBoldTextView
            android:id="@+id/discontinue_membership"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/yello_rectangle_shape"
            style="@style/button_text_style"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:text="Discontinue Membership"
            android:visibility="gone"
            />

        <in.woloo.www.application_kotlin.utilities.CenturyGoticBoldTextView
            android:id="@+id/delete_user"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/pink_rectanlge_shape"
            style="@style/button_text_style"
            android:text="Delete User"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_70sdp"
            android:visibility="gone"
            />

    </LinearLayout>
    </ScrollView>




</LinearLayout>
